<idea-plugin>
    <id>com.github.developerutils.kotlin-function-arguments-helper</id>
    <name>Kotlin Function Arguments Helper</name>
    <version>2020.1.1</version>
    <vendor email="ch.graupner@workingdeveloper.net" url="https://github.com/DeveloperUtils">WorkingDeveloper</vendor>

    <description><![CDATA[
        <p>Intellij plugin that provide intention action for constructor or function to fill arguments with default value.</p>
        <a target="_blank" href="https://github.com/DeveloperUtils/kotlin-function-arguments-helper/tree/master/docs/kotlin-arguments-helper-demo.gif" alt="fill-class" style="max-width:100%;"></a></p>
    ]]></description>

<!--    <change-notes><![CDATA[-->
<!--    Add support for IntelliJ 2020.1-->
<!--    ]]>-->
<!--    </change-notes>-->

    <!-- please see https://confluence.jetbrains.com/display/IDEADEV/Build+Number+Ranges for description -->
    <idea-version since-build="192" until-build="203.*"/>

    <depends>com.intellij.modules.lang</depends>
    <depends>org.jetbrains.kotlin</depends>

    <extensions defaultExtensionNs="com.intellij">
        <intentionAction>
            <className>com.github.developerutils.kotlinfunctionargumentshelper.KotlinFunctionArgumentsHelperIntention</className>
            <category>Kotlin</category>
        </intentionAction>
    </extensions>

    <application-components>
    </application-components>

    <project-components>
    </project-components>
</idea-plugin>
